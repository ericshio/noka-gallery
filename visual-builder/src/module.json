{
  "name": "noka/gallery-module",
  "slug": "noka-gallery-module",
  "d4Shortcode": "noka_gallery_module",
  "title": "Noka Gallery",
  "titles": "Noka Galleries",
  "category": "module",
  "moduleClassName": "noka_gallery_module",
  "moduleOrderClassName": "noka_gallery_module",
  "attributes": {
    "module": {
      "type": "object",
      "selector": "{{selector}}",
      "default": {
        "meta": { "adminLabel": { "desktop": { "value": "Noka Gallery" } } }
      },
      "settings": {
        "meta": { "adminLabel": {} },
        "decoration": { "background": {}, "border": {}, "boxShadow": {}, "disabledOn": {}, "position": {}, "zIndex": {} }
      }
    },
    "gallery_select": {
      "type": "object",
      "selector": "{{selector}} .noka-gallery-module",
      "default": {
        "innerContent": {
          "desktop": {
            "value": "none"
          }
        }
      },
      "settings": {
        "innerContent": {
          "groupType": "group-item",
          "item": {
            "groupName": "mainContent",
            "priority": 10,
            "render": true,
            "attrName": "gallery_select.innerContent",
            "label": "Select Gallery",
            "description": "Choose the gallery to display.",
            "component": {
              "name": "divi/select",
              "type": "field"
            }
          }
        }
      }
    }
  },
  "settings": {
    "content": "auto",
    "design": "auto",
    "advanced": "auto"
  }
}